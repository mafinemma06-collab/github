<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Samiha.Editor.EditorDashboardController">

    <children>

        <ButtonBar prefHeight="40.0" prefWidth="200.0" />

        <Button layoutX="52.0" layoutY="59.0"
                mnemonicParsing="false"
                onAction="#sendFeedbackOnaction"
                text="sendFeedback" />

        <Button layoutX="52.0" layoutY="104.0"
                mnemonicParsing="false"
                onAction="#removeOnaction"
                text="remove" />

        <Button layoutX="52.0" layoutY="144.0"
                mnemonicParsing="false"
                onAction="#approveorrejectreadercommentsOnAction"
                text="approve or reject reader comments" />

        <Button layoutX="52.0" layoutY="188.0"
                mnemonicParsing="false"
                onAction="#archiveOnAction"
                text="Archive" />

        <Button layoutX="52.0" layoutY="238.0"
                mnemonicParsing="false"
                onAction="#searchOnaction"
                text="search" />

        <Button layoutX="310.0" layoutY="65.0"
                mnemonicParsing="false"
                onAction="#reviewOnaction"
                text="review" />
        <Button layoutX="310.0" layoutY="104.0" text="Button" />
        <Button layoutX="310.0" layoutY="144.0" text="Button" />

        <AnchorPane fx:id="partialPane"
                    layoutX="200.0"
                    layoutY="20.0"
                    prefWidth="380"
                    prefHeight="360"
                    style="-fx-border-color: black; -fx-border-width: 1;">
        </AnchorPane>

    </children>
</AnchorPane>